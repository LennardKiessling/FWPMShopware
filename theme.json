        "type": "text",
        "value": "14px",
        "editable": true,
        "tab": "sizes",
        "block": "fontSizes"
      },
      "LFL-font-size-base": {
        "label":{
          "en-GB": "Font size of base",
          "de-DE": "Schriftgröße Basis"
        },
        "type": "text",
        "value": "1.125rem",
        "editable": true,
        "tab": "sizes",
        "block": "fontSizes"
      },
      "LFL-font-size-lg": {
        "label":{
          "en-GB": "Font size of large",
