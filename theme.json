              }
            },
            {
              "value": "minimal",
              "label": {
                "en-GB": "Minimales Layout",
                "de-DE": "Minimales Layout"
              }
            }
          ]
        },
        "editable": true,
        "tab": "general-settings"
      },
      "uvp-in-header-allow": {
        "label":{
          "en-GB": "Show UVP in header On/Off",
          "de-DE": "Zeig UVP im Header An/Aus"
        },
        "type": "switch",
        "value": "true",
        "editable": true,
