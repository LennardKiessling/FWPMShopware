      },
      "uvp-icon-in-header-right": {
        "label":{
          "en-GB": "UVP Icon Header Right",
          "de-DE": "UVP Icon Header Rechts"
        },
        "type": "text",
        "value": "address",
        "editable": true,
        "tab": "header-settings"
      },
      "uvp-text-in-header-right": {
        "label":{
          "en-GB": "UVP Text Header Left",
          "de-DE": "UVP Text Header Links"
        },
        "type": "text",
        "value": "Geldzurück Garantie!",
        "editable": true,
        "tab": "header-settings"
      },
      "uvp-background-color": {
        "label":{
          "en-GB": "UVP Background Color",
          "de-DE": "UVP Hintergrund Farbe"
        },
        "type": "color",
        "value": "#000000",
        "editable": true,
        "tab": "header-settings",
        "block": "uvp-colors"
