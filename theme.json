  "config": {

    "tabs": {
      "sizes": {
          "label": {
              "en-GB": "Sizesettings",
              "de-DE": "Größeneinstellungen"
          }
      },
      "general-settings": {
        "label": {
            "en-GB": "General Settings",
            "de-DE": "Allgemeine Einstellungen"
        }
      },
      "colors": {
        "label": {
            "en-GB": "Colors",
            "de-DE": "Farbeinstellungen"
        }
      },
      "header-settings": {
        "label": {
            "en-GB": "Header Settings",
            "de-DE": "Header und Navi Einstellungen"
        }
      }
    },

    "blocks": {
      "sizes": {
        "fontSizes": {
          "en-GB": "Font Sizes",
          "de-DE": "Schriftgrößen"
        },
        "fontWeights": {
          "en-GB": "Font Weight",
          "de-DE": "Schriftstärke"
        }
      },
      "header-settings": {
        "header-colors": {
          "en-GB": "Header Colors",
          "de-DE": "Farben Header"
        },
        "header-sizes": {
          "en-GB": "Sizes Header",
          "de-DE": "Header Größen"
        },
        "uvp-colors": {
          "en-GB": "Sizes Header",
          "de-DE": "Header Größen"
        }
      },
      "color-settings": {
        "footer-colors": {
          "en-GB": "Footer Colors",
          "de-DE": "Farben Footer"
        }
      }
    },

    "fields": {
      "site-max-width": {
        "label": {
          "en-GB": "Width of the Website",
          "de-DE": "Breite der Website"
        },
        "type": "text",
        "value": "100%",
        "editable": true,
        "tab": "sizes"
      },
      "display-home-button": {
        "label":{
          "en-GB": "Display Home Button in Navigation On/Off",
          "de-DE": "Zeige den Home Button in der Navigation An/Aus"
        },
        "type": "switch",
        "value": false,
        "editable": true,
        "tab": "general-settings"
      },
      "border-radius-product-box": {
        "label":{
          "en-GB": "Border Radius of the product box",
          "de-DE": "Rundung des Rahmens der Produktbox"
        },
        "type": "text",
        "value": "10px",
        "editable": true,
        "tab": "sizes"
      },
      "padding-product-box": {
        "label":{
          "en-GB": "padding of the product box",
          "de-DE": "Innenabstand der Produktbox"
        },
        "type": "text",
        "value": "0px",
        "editable": true,
        "tab": "sizes"
      },
      "border-color-product-box": {
        "label":{
          "en-GB": "Border Color of the Product Box",
          "de-DE":"Farbe der Border der Product Box"
        },
        "type": "color",
        "value": "lightgrey",
        "editable": true,
        "tab": "colors"
      },
      "checkout-background-color": {
        "label":{
          "en-GB": "Background color of the checkout",
          "de-DE":"Hintergrundfarbe des Checkouts"
        },
        "type": "color",
        "value": "#E8E8E8",
        "editable": true,
        "tab": "colors"
      },
      "checkout-background-elements-color": {
        "label":{
          "en-GB": "Background color of the elements used in the checkout",
          "de-DE":"Hintergrundfarbe der einzelnen Elemente im Checkout"
        },
        "type": "color",
        "value": "#f9f9f9",
        "editable": true,
        "tab": "colors"
      },
      "border-radius-checkout": {
        "label":{
          "en-GB": "Border Radius of the checkout elements",
          "de-DE": "Rundung des Rahmens der Checkout Elemente"
        },
        "type": "text",
        "value": "15px",
        "editable": true,
        "tab": "sizes"
      },
      "btn-border-radius": {
        "label":{
          "en-GB": "Border Radius of the Buttons",
          "de-DE": "Rundung des Rahmens der Buttons"
        },
        "type": "text",
        "value": "5px",
        "editable": true,
        "tab": "sizes"
      },
      "card-border-radius": {
        "label":{
          "en-GB": "Border Radius of the Account Cards",
          "de-DE": "Rundung des Rahmens der Account Kartenabschnitte"
